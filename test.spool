main {
    const greeter: Greeter = new Greeter("Hello!")
    const random: spool.core.Random = new spool.core.Random()
    const number: spool.core.number.Int16 = random.nextInt16(0, 5)
    const console: spool.core.Console = new spool.core.Console()
    console.println(number)
    greeter.greet()
}

class Greeter {
    const greeting: spool.core.String

    constructor(greeting: spool.core.String) {
        self.greeting = greeting
    }

    func greet() {
        const console: spool.core.Console = new spool.core.Console()
        console.println(self.greeting)
    }
}