main {
    const console: spool.core.Console = new spool.core.Console()

    console.println("Testing pow")
    console.println(2^3)

    console.println("Testing negation.")
    console.println(-2)

    console.println("Testing less than.")
    console.println(1 < 2)

    console.println("Testing greater than.")
    console.println(1 > 2)

    console.println("Testing equals")
    console.println(1 == 1)

    console.println("Testing less than equals")
    console.println(1 <= 2)

    console.println("Testing greater than equals")
    console.println(1 >= 2)

    console.println("Testing not equal")
    console.println(1 != 2)

    console.println("Testing logic negation")
    const value: spool.core.Boolean = 1 == 1
    console.println(!value)

    console.println("Testing and")
    console.println(1 == 1 and 1 == 2)

    console.println("Testing or")
    console.println(1 == 1 or 1 == 2)
}